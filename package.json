{
  "name": "vk-bot-for-download-photo-chats",
  "version": "1.0.0",
  "description": "Typescript",
  "scripts": {
    "start": "ts-node-dev --respawn src/index.ts",
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "eslint-check": "npx eslint src/index.ts",
    "eslint-fix": "npx eslint src/index.ts --fix",
    "prettier-start": "npx prettier --single-quote --tab-width 2 src/index.ts",
    "prettier-clean": "npx prettier src/index.ts",
    "prettier-write": "prettierrc --write src/index.ts"
  },
  "keywords": [
    "typescript",
    "tsc",
    "javascript",
    "NodeJS",
    "starter",
    "kit"
  ],
  "author": "Ckieverentineas",
  "devDependencies": {
    "@types/node": "^13.13.52",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "nodemon": "^2.0.2",
    "prettier": "1.19.1",
    "ts-node": "^8.10.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "typescript": "^3.7.5"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "https": "^1.0.0",
    "vk-io": "^4.8.3"
  }
}
